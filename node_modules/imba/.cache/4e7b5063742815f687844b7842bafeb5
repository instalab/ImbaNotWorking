var Imba = require('imba');
var Widget = require('./widget').Widget;

function ExplorerPanel(){ return Widget.apply(this,arguments) };

Imba.subclass(ExplorerPanel,Widget);
exports.ExplorerPanel = ExplorerPanel; // export class 
ExplorerPanel.register('explorer');

ExplorerPanel.prototype.view = function(v){ return this._view; }
ExplorerPanel.prototype.setView = function(v){ this._view = v; return this; };

ExplorerPanel.prototype.setup = function (){
	return this;
};
