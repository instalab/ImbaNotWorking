var Imba = require('imba');
var Resource$ = require('../store/Resource'), Resource = Resource$.Resource, MASK = Resource$.MASK, ACCESS = Resource$.ACCESS;

function Membership(){ return Resource.apply(this,arguments) };

Imba.subclass(Membership,Resource);
exports.Membership = Membership; // export class 
Membership.prototype.user = function (){
	return this.db().get(this.uid);
};

Membership.prototype.group = function (){
	return this.db().get(this.gid);
};

Membership.prototype.dependsOn = function (){
	return [this.user(),this.group()];
};
