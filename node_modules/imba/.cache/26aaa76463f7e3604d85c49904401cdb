var Imba = require('imba'), _1 = Imba.createElement;
var UserLinks = Imba.defineTag('UserLinks', function(tag){
	tag.prototype.render = function (){
		var $ = this.$;
		return this.$open(0).setChildren([
			this.data().username ? (
				($[0] || _1('span',$,0,this).flag('profile').setContent($[1] || _1('a',$,1,0),2)).end((
					$[1].setHref(this.data().url()).setText("@" + this.data().username).end()
				,true))
			) : void(0),
			(this.data().github_handle || this.data().username) ? (
				($[2] || _1('span',$,2,this).flag('gh').setContent($[3] || _1('a',$,3,2).dataset('icon','github').setTarget('blank').setContent($[4] || _1('span',$,4,3).setText("github"),2),2)).end((
					$[3].setHref(("https://github.com/" + (this.data().github_handle || this.data().username))).end()
				,true))
			) : void(0),
