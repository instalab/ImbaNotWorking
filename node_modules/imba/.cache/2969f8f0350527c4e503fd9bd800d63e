var Imba = require('imba');
var Widget = require('./widget').Widget;

function LayoutWidget(){ return Widget.apply(this,arguments) };

Imba.subclass(LayoutWidget,Widget);
exports.LayoutWidget = LayoutWidget; // export class 
LayoutWidget.register('layout');

LayoutWidget.option('renderWhitespace',{'default': false,overridable: true});
LayoutWidget.option('viewport',{type: Array,overridable: true});

LayoutWidget.prototype.setup = function (){
	return this;
};

LayoutWidget.prototype.deserialized = function (){
	return this;
};
