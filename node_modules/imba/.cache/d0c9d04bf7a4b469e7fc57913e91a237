var Imba = require('imba');
var KarmaCounter = Imba.defineTag('KarmaCounter', function(tag){
	tag.prototype.user = function (){
		return this.data();
	};
	
	tag.prototype.render = function (){
		return this.$open(0).setChildren(this.data().karma || '',3).synced();
	};
})
exports.KarmaCounter = KarmaCounter;
